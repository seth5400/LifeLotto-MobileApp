<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".question_reg4">


    <TextView
        android:id="@+id/textView"
        android:layout_width="366dp"
        android:layout_height="69dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="23dp"
        android:fontFamily="@font/eazy_chat"
        android:text="@string/ques4_title"
        android:textColor="#0cc0df"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/cont1_btn"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/btn_continue"
        android:fontFamily="@font/eazy_chat"
        android:letterSpacing="0.02"
        android:paddingTop="5dp"
        android:text="@string/ques4_btn"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="55dp"
        android:layout_height="8dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="319dp"
        android:background="@drawable/btn_order"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="55dp"
        android:layout_height="8dp"
        android:layout_marginStart="106dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="251dp"
        android:background="@drawable/btn_order"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="55dp"
        android:layout_height="8dp"
        android:layout_marginStart="174dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="182dp"
        android:background="@drawable/btn_order"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="55dp"
        android:layout_height="8dp"
        android:layout_marginStart="245dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="111dp"
        android:background="@drawable/btn_order"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="55dp"
        android:layout_height="8dp"
        android:layout_marginStart="318dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="38dp"
        android:background="@drawable/btn_order2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btn_meat"
        android:layout_width="340dp"
        android:layout_height="70dp"
        android:layout_marginTop="45dp"
        android:background="@drawable/btn_question"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:layout_marginStart="263dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="37dp"
            android:layout_marginBottom="10dp"
            android:src="@drawable/icon_male"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView72"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/eazy_chat"
            android:text="@string/ques4_text1"
            android:textColor="#0cc0df"
            android:textSize="28dp"
            tools:layout_editor_absoluteX="34dp"
            tools:layout_editor_absoluteY="20dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btn_vegetarian"
        android:layout_width="340dp"
        android:layout_height="70dp"
        android:layout_marginTop="45dp"
        android:background="@drawable/btn_question"
        app:layout_constraintEnd_toEndOf="@+id/btn_meat"
        app:layout_constraintStart_toStartOf="@+id/btn_meat"
        app:layout_constraintTop_toBottomOf="@+id/btn_meat">

        <ImageView
            android:id="@+id/imageView74"
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:layout_marginStart="263dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="37dp"
            android:layout_marginBottom="10dp"
            android:src="@drawable/icon_female"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView127"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/eazy_chat"
            android:text="@string/ques4_text2"
            android:textColor="#0cc0df"
            android:textSize="28dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="34dp"
            tools:layout_editor_absoluteY="20dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btn_both"
        android:layout_width="340dp"
        android:layout_height="70dp"
        android:layout_marginTop="45dp"
        android:background="@drawable/btn_question"
        app:layout_constraintEnd_toEndOf="@+id/btn_vegetarian"
        app:layout_constraintStart_toStartOf="@+id/btn_vegetarian"
        app:layout_constraintTop_toBottomOf="@+id/btn_vegetarian">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:layout_marginStart="263dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="37dp"
            android:layout_marginBottom="10dp"
            android:src="@drawable/icon_other"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/eazy_chat"
            android:text="@string/ques4_text3"
            android:textColor="#0cc0df"
            android:textSize="28dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="34dp"
            tools:layout_editor_absoluteY="20dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>